}
.template .content .tuiJian .list .item .control view text,
.template .content .reMen .list .item .control view text,
.template .content .wenDu .list .item .control view text,
.template .content .collect .list .item .control view text {
  color: #b2b2b2;
  font-size: 20rpx;
  margin-left: 9rpx;
}
.template .content .tuiJian .list .item .control view:first-of-type,
.template .content .reMen .list .item .control view:first-of-type,
.template .content .wenDu .list .item .control view:first-of-type,
.template .content .collect .list .item .control view:first-of-type {
  margin-left: 0;
}
.template .content .tuiJian .list .item .control view:first-of-type image,
.template .content .reMen .list .item .control view:first-of-type image,
.template .content .wenDu .list .item .control view:first-of-type image,
.template .content .collect .list .item .control view:first-of-type image {
  width: 32rpx;
  height: 28rpx;
}
.template .content .tuiJian .list .item .control view:nth-of-type(2) image,
.template .content .reMen .list .item .control view:nth-of-type(2) image,
.template .content .wenDu .list .item .control view:nth-of-type(2) image,
.template .content .collect .list .item .control view:nth-of-type(2) image {
  width: 32rpx;
  height: 32rpx;
}
.template .content .tuiJian .list .item .control view:nth-of-type(3) image,
.template .content .reMen .list .item .control view:nth-of-type(3) image,
.template .content .wenDu .list .item .control view:nth-of-type(3) image,
.template .content .collect .list .item .control view:nth-of-type(3) image {
  width: 31rpx;
  height: 30rpx;
}
.template .content .tuiJian .swiper-box {
  height: 395rpx;
  background: #eeeeee;
  position: relative;
}
.template .content .collect .cancel {
  height: 56rpx;
  font-size: 0;
  text-align: right;
  padding: 0 6.4%;
  border-bottom: 1px solid #f2f2f2;
}
.template .content .collect .cancel text {
  line-height: 56rpx;
  font-size: 20rpx;
}
.template .content .collect .cancel text:first-of-type {
  color: #b2b2b2;
}
.template .content .collect .cancel text:last-of-type {
  color: #2ca5ae;
}
.template .content .collect .cancel view {
  display: inline-block;
  margin: 0 17rpx;
  width: 2px;
  height: 15rpx;
  background: #b2b2b2;
}
.template .fixed-bottom {
  background: #f2f2f2;
  height: 58rpx;
  line-height: 58rpx;
  width: 100%;
  position: fixed;
  left: 0;
  bottom: 0;
  font-size: 17rpx;
  color: #797878;
  text-align: center;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
