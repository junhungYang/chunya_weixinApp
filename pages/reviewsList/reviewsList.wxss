.template .list .item {
  padding: 0 6.4%;
  border-bottom: 1px solid #f2f2f2;
}
.template .list .item .info .my-info {
  padding: 20rpx 0;
  display: flex;
}
.template .list .item .info .my-info image {
  width: 58rpx;
  height: 58rpx;
  border-radius: 50%;
}
.template .list .item .info .my-info text {
  margin-left: 10rpx;
  line-height: 58rpx;
  color: #313131;
  font-size: 24rpx;
}
.template .list .item .info .time-info {
  margin-top: 9rpx;
  line-height: 20rpx;
  font-size: 20rpx;
  color: #b7b7b7;
}
.template .list .item .info .time-info text:nth-of-type(2) {
  margin-left: 10rpx;
}
.template .list .item .cont {
  margin-top: 23rpx;
  margin-bottom: 30rpx;
}
.template .list .item .cont .text {
  font-size: 28rpx;
  line-height: 38rpx;
}
.template .list .item .cont .pic-group {
  font-size: 0;
}
.template .list .item .cont .pic-group image {
  margin: 14rpx 7rpx 0 7rpx;
  width: 204rpx;
}
