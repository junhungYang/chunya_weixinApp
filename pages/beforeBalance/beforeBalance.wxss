.template {
  background: #f7f7f7;
  height: 100vh;
}
.template .about-address {
  height: 120rpx;
  background: #fff;
  padding: 0 6.4%;
}
.template .about-address .add {
  height: 100%;
  display: flex;
}
.template .about-address .add image:first-of-type {
  width: 32rpx;
  height: 32rpx;
  align-self: center;
}
.template .about-address .add text {
  flex: 1;
  padding-left: 21rpx;
  line-height: 120rpx;
  font-size: 32rpx;
  color: #313131;
}
.template .about-address .add image:last-of-type {
  width: 13rpx;
  height: 24rpx;
  align-self: center;
}
.template .desc {
  margin-top: 20rpx;
  background: #fff;
}
.template .desc .header {
  height: 84rpx;
  border-bottom: 2px solid #f7f7f7;
  line-height: 84rpx;
  padding: 0 6.4%;
}
.template .desc .header image {
  width: 44rpx;
  height: 44rpx;
  vertical-align: middle;
}
.template .desc .header text {
  margin-left: 23rpx;
  font-size: 28rpx;
  color: #313131;
}
.template .desc .good {
  display: flex;
  height: 180rpx;
  margin: 0 6.4%;
  border-bottom: 2px solid color #f7f7f7;
}
.template .desc .good image {
  width: 124rpx;
  height: 124rpx;
  align-self: center;
}
.template .desc .good view {
  flex: 1;
  align-self: center;
  padding-left: 23rpx;
  position: relative;
}
.template .desc .good view text {
  display: block;
}
.template .desc .good view text:first-of-type {
  font-size: 28rpx;
  color: #313131;
  width: 100%;
  white-space: nowrap ;
  overflow: hidden;
  text-overflow: ellipsis;
}
.template .desc .good view text:nth-of-type(2) {
  margin-top: 23rpx;
  font-size: 28rpx;
  color: #d1051f;
}
.template .desc .good view text:last-of-type {
  position: absolute;
  right: 0;
  top: 10rpx;
  font-size: 24rpx;
  color: #B2B2B2;
}
